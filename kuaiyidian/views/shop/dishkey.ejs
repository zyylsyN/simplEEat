<% include ./shoptop %>
	<div class="row">
          <div class="col-sm-2"></div>
          <div class="col-sm-8" style='margin:auto;text-align: center;'>
                <form action='./addDishkey' method='post'>
                	<table class="table table-bordered" align='center'>
				  <tbody>
				  	<tr>
				  		<td colspan="2" align='center'>
				  			<%-dishname%>
				  		</td>
				  	</tr>
				    <tr>
				      <td>速查关键词:</td>
				      <td>
				      	<input type="text" class="form-control" placeholder="请输入菜品速查字符" name='dishkey'>
				      </td>
				    </tr>
				    <tr>
				      <td colspan="2" align='center'>
				      	<input type='hidden' name='dishid' value='<%-dishid%>'/>
				      	<input type='hidden' name='dishname' value='<%-dishname%>'/>
				      	<input type='submit' value='提交' class='btn btn-primary'/>
				      </td>
				    </tr>
				    
				  </tbody>
				</table>
                </form>
          </div>
          <div class="col-sm-2"></div>
        </div>

        <div class="row">
          <div class="col-sm-2"></div>
          <div class="col-sm-8" style='margin:auto;text-align: center;'>
                <table class="table table-bordered" align='center'>
				  <tbody>
				  	<tr>
				  		<td colspan="2" align='center'>
				  			<%-dishname%>
				  		</td>
				  	</tr>
				  	<tr align='center'>
				  		<td>速查字符</td>
				  		<td>操作</td>
				  	</tr>
				  	<% rs.forEach(function(item){ %>
				  	<tr align='center'>
				  		<td><%-item.dishkey%></td>
				  		<td>
				  			修改&nbsp;
				  			删除
				  		</td>
				  	</tr>
				  	<%})%>
				  </tbody>
				</table>
          </div>
          <div class="col-sm-2"></div>
        </div>
</body>
</html>